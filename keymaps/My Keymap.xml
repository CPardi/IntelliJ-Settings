<keymap version="1" name="My Keymap" parent="Visual Studio">
  <action id="RecentFiles" />
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="alt f8" />
    <keyboard-shortcut first-keystroke="ctrl alt f" />
    <keyboard-shortcut first-keystroke="ctrl e" second-keystroke="ctrl c" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="Vcs.ShowMessageHistory">
    <keyboard-shortcut first-keystroke="ctrl m" />
  </action>
</keymap>